{
    "name":"Web Capture",
    "version":"0.0.0.1",
    "manifest_version":2,
    "author": "ritesh Jaiswal",
    "description": "A browser extension to take screenshots and save it",
    "icons":{
        "64": "icons/icon1.png",
        "128": "icons/icon2.png",
        "256": "icons/icon3.png"
         },
    "browser_action":{
      "default_popup":"popup/popup.html"
       },
    "permissions":[
      "activeTab"
    ]

}